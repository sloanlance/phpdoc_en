<section id="iconv.configuration">
 &reftitle.runtime;
 &extension.runtime;
 <para>
 <table>
  <title>Iconv configuration options</title>
  <tgroup cols="3">
   <thead>
    <row>
     <entry>Name</entry>
     <entry>Default</entry>
     <entry>Changeable</entry>
    </row>
   </thead>
   <tbody>
     <row>
      <entry>iconv.input_encoding</entry>
      <entry>ICONV_INPUT_ENCODING</entry>
      <entry>PHP_INI_ALL</entry>
     </row>
     <row>
      <entry>iconv.output_encoding</entry>
      <entry>ICONV_OUTPUT_ENCODING</entry>
      <entry>PHP_INI_ALL</entry>
     </row>
     <row>
      <entry>iconv.internal_encoding</entry>
      <entry>ICONV_INTERNAL_ENCODING</entry>
      <entry>PHP_INI_ALL</entry>
     </row>
   </tbody>
  </tgroup>
 </table>
 &ini.php.constants;
 </para>
 <note>
  <para>
   Configuration option <parameter>iconv.input_encoding</parameter> is
   currently not used for anything.
  </para>
 </note>
</section>

