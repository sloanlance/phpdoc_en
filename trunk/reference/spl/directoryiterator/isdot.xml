<refentry xml:id="directoryiterator.isdot" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>DirectoryIterator::isDot</refname>
  <refpurpose>Returns true if current entry is '.' or '..'</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>bool</type><methodname>DirectoryIterator::isDot</methodname>
   <void/>
  </methodsynopsis>

   &warn.undocumented.func;

  <para>
   Check whether it's a directory and either <literal>.</literal>
   or <literal>..</literal>.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &true; if the entry is <literal>.</literal> or <literal>..</literal>,
   otherwise &false;
  </para>
 </refsect1>

</refentry>

