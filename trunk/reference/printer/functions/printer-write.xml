<refentry xml:id="function.printer-write" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>printer_write</refname>
  <refpurpose>Write data to the printer</refpurpose>
 </refnamediv>
 
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>bool</type><methodname>printer_write</methodname>
   <methodparam><type>resource</type><parameter>printer_handle</parameter></methodparam>
   <methodparam><type>string</type><parameter>content</parameter></methodparam>
  </methodsynopsis>
  <para>
   Writes <parameter>content</parameter> directly to the printer.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>printer_handle</parameter></term>
     <listitem>
      <para>
       <parameter>printer_handle</parameter> must be a valid printer handle.
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>content</parameter></term>
     <listitem>
      <para>
       The data to be written.
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.success;
  </para>
 </refsect1>

 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title><function>printer_write</function> example</title>
    <programlisting role="php">
<![CDATA[
<?php
$handle = printer_open();
printer_write($handle, "Text to print");
printer_close($handle);
?>
]]>
    </programlisting>
   </example>
  </para>
 </refsect1>
 
</refentry>

