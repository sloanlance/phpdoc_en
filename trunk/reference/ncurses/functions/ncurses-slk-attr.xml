<refentry xml:id="function.ncurses-slk-attr" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>ncurses_slk_attr</refname>
  <refpurpose>Returns current soft label key attribute</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>int</type><methodname>ncurses_slk_attr</methodname>
   <void/>
  </methodsynopsis>
  &warn.experimental.func;
  <para>
   Returns the current soft label key attribute.
  </para>
 </refsect1>
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   The attribute, as an integer.
  </para>
 </refsect1>
</refentry>

