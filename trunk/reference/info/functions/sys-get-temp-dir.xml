<refentry id="function.sys-get-temp-dir">
 <refnamediv>
  <refname>sys_get_temp_dir</refname>
  <refpurpose>Returns directory path used for temporary files</refpurpose>
 </refnamediv>
 
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>string</type><methodname>sys_get_temp_dir</methodname>
   <void/>
  </methodsynopsis>
  <para>
   Returns the path of the directory PHP stores temporary files in by
   default.
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Returns the path of the temporary directory.
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><function>tmpfile</function></member>
    <member><function>tempnam</function></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>

