<refentry xml:id="function.cpdf-setlinejoin" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>cpdf_setlinejoin</refname>
  <refpurpose>Sets linejoin parameter</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>bool</type><methodname>cpdf_setlinejoin</methodname>
   <methodparam><type>int</type><parameter>pdf_document</parameter></methodparam>
   <methodparam><type>int</type><parameter>value</parameter></methodparam>
  </methodsynopsis>
  <para>
   Set the linejoin parameter.
  </para>
 </refsect1>
 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    &cpdf.pdf-document.description;
    <varlistentry>
     <term><parameter>value</parameter></term>
     <listitem>
      <para>
       0 = miter, 1 = round, 2 = bevel
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.success;
  </para>
 </refsect1>
</refentry>

